<li class="test {{status}}">
	{{> actions}}
	<div class="status"></div>
	{{{stringFormat}}}
	{{#unless string}}&nbsp;{{/unless}}
	{{#if match}}
		<div class="matches">
			<strong class="pull-left">Captures</strong>
			<ol class="inline">
			{{#rest match}}
				<li>{{this}}</li>
			{{/rest}}
			</ol>
		</div>
	{{/if}}
</li>
